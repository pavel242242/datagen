{
  "version": "1.0",
  "metadata": {
    "name": "E-commerce Platform",
    "description": "E-commerce dataset with users, products, and orders - Generated by Claude"
  },
  "timeframe": {
    "start": "2024-01-01T00:00:00Z",
    "end": "2024-12-31T23:59:59Z",
    "freq": "H"
  },
  "constraints": {
    "foreign_keys": [
      {"from": "order.user_id", "to": "user.user_id"},
      {"from": "order_item.order_id", "to": "order.order_id"},
      {"from": "order_item.product_id", "to": "product.product_id"}
    ]
  },
  "nodes": [
    {
      "id": "user",
      "kind": "entity",
      "pk": "user_id",
      "rows": 500,
      "columns": [
        {
          "name": "user_id",
          "type": "int",
          "generator": {"sequence": {"start": 1, "step": 1}}
        },
        {
          "name": "email",
          "type": "string",
          "generator": {"faker": {"method": "email"}}
        },
        {
          "name": "name",
          "type": "string",
          "generator": {"faker": {"method": "name"}}
        },
        {
          "name": "signup_date",
          "type": "datetime",
          "generator": {
            "datetime_series": {
              "within": {"start": "2024-01-01T00:00:00Z", "end": "2024-12-31T23:59:59Z"},
              "freq": "D"
            }
          }
        },
        {
          "name": "country",
          "type": "string",
          "generator": {
            "choice": {
              "choices": ["US", "UK", "CA", "AU", "DE"],
              "weights": [0.4, 0.2, 0.15, 0.15, 0.1]
            }
          }
        }
      ]
    },
    {
      "id": "product",
      "kind": "entity",
      "pk": "product_id",
      "rows": 200,
      "columns": [
        {
          "name": "product_id",
          "type": "int",
          "generator": {"sequence": {"start": 1000, "step": 1}}
        },
        {
          "name": "product_name",
          "type": "string",
          "generator": {"faker": {"method": "word"}}
        },
        {
          "name": "category",
          "type": "string",
          "generator": {
            "choice": {
              "choices": ["Electronics", "Clothing", "Home", "Books", "Toys"],
              "weights": [0.3, 0.25, 0.2, 0.15, 0.1]
            }
          }
        },
        {
          "name": "price",
          "type": "float",
          "generator": {
            "distribution": {
              "type": "lognormal",
              "params": {"mu": 3.5, "sigma": 0.8},
              "clamp": [9.99, 999.99]
            }
          }
        }
      ]
    },
    {
      "id": "order",
      "kind": "fact",
      "pk": "order_id",
      "parents": ["user"],
      "fanout": {"distribution": "poisson", "lambda": 3.5},
      "columns": [
        {
          "name": "order_id",
          "type": "int",
          "generator": {"sequence": {"start": 10000, "step": 1}}
        },
        {
          "name": "user_id",
          "type": "int",
          "generator": {"lookup": {"from": "user.user_id"}}
        },
        {
          "name": "order_date",
          "type": "datetime",
          "generator": {
            "datetime_series": {
              "within": {"start": "2024-01-01T00:00:00Z", "end": "2024-12-31T23:59:59Z"},
              "freq": "H"
            }
          }
        },
        {
          "name": "status",
          "type": "string",
          "generator": {
            "choice": {
              "choices": ["pending", "processing", "shipped", "delivered", "cancelled"],
              "weights": [0.05, 0.1, 0.2, 0.6, 0.05]
            }
          }
        },
        {
          "name": "total_amount",
          "type": "float",
          "generator": {
            "distribution": {
              "type": "lognormal",
              "params": {"mu": 4.2, "sigma": 0.6},
              "clamp": [20.0, 2000.0]
            }
          },
          "modifiers": [
            {
              "transform": "jitter",
              "args": {"std": 0.05, "mode": "mul"}
            }
          ]
        }
      ]
    },
    {
      "id": "order_item",
      "kind": "fact",
      "pk": "order_item_id",
      "parents": ["order"],
      "fanout": {"distribution": "poisson", "lambda": 2.5},
      "columns": [
        {
          "name": "order_item_id",
          "type": "int",
          "generator": {"sequence": {"start": 1, "step": 1}}
        },
        {
          "name": "order_id",
          "type": "int",
          "generator": {"lookup": {"from": "order.order_id"}}
        },
        {
          "name": "product_id",
          "type": "int",
          "generator": {"lookup": {"from": "product.product_id"}}
        },
        {
          "name": "quantity",
          "type": "int",
          "generator": {
            "distribution": {
              "type": "poisson",
              "params": {"lambda": 1.8},
              "clamp": [1, 10]
            }
          }
        },
        {
          "name": "unit_price",
          "type": "float",
          "generator": {
            "distribution": {
              "type": "lognormal",
              "params": {"mu": 3.5, "sigma": 0.8},
              "clamp": [9.99, 999.99]
            }
          }
        }
      ]
    }
  ]
}
